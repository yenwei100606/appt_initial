        <activity
            android:name=".trend"
            android:screenOrientation="portrait" />
        <activity
            android:name=".uploadconfirm"
            android:screenOrientation="portrait" />
        <activity
            android:name=".detail"
            android:screenOrientation="portrait" />
        <activity
            android:name=".uploadconfirmURS"
            android:screenOrientation="portrait" />
        <activity
            android:name=".replaceURS"
